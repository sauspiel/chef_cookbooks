default: &mysql
  adapter: mysql2
  pool: 5
  timeout: 5000
  username: <%= @user %>
  password: <%= @password %>

development:
  <<: *mysql
  database: team_dashboard_development

test:
  <<: *mysql
  database: team_dashboard_test

staging:
  <<: *mysql
  database: team_dashboard_staging

production:
  <<: *mysql
  database: team_dashboard_production
